<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Hola</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>SrvHola</display-name>
		<servlet-name>SrvHola</servlet-name>
		<servlet-class>hola.servlets.SrvHola</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SrvHola</servlet-name>
		<url-pattern>/SrvHola</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>SrvCicloVida</display-name>
		<servlet-name>SrvCicloVida</servlet-name>
		<servlet-class>com.ipartek.hola.servlet.SrvCicloVida</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>SrvCicloVida</servlet-name>
		<url-pattern>/cicloVida</url-pattern>
	</servlet-mapping>
</web-app>