package empresa;

public abstract class Empleado extends Persona {
	
	private static int CodEmple=0;
	
	public Empleado(int num, char letra,int edad, String ...NombreCompleto){
		super(edad,num,letra,NombreCompleto);
		CodEmple++;
		
	}
	public String toString(){
		
		return super.toString()+""+super.super.toString()+" SU CODIGO DE EMPLEADO ES: "+CodEmple;
	}

}
