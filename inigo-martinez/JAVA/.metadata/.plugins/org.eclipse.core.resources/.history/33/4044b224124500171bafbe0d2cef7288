package tragaperras;

public class Premio {

	//String arrayPremios[] = {"fresa", "sandía", "plátano", "melocotón","pera"};
	String arrayPremios[] = {"100€","200€","una patada en la cara", "un dia en el SPA", "un viaje de bus a tu casa"};
	
	public String resolver(String ...x){
		
		if(x[0].equals(x[1])&& x[0].equals(x[2])){
			
			return arrayPremios[(int)Math.random()*4];
			
		}
		return "";
		
	}

}
