package prueba;

public class Orco {

	int PV, danio, armadura;

	public Orco(int pV, int danio, int armadura) {
		this.PV = pV;
		this.danio = danio;
		this.armadura = armadura;
	}

	public int atacar() {

		int danioRealizado = Integer.parseInt(Double.toString((Math.random() * (this.danio))));
		return danioRealizado;

	}

}
