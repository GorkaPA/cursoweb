package paquete1;

public class EjecutarOpciones {

	public static void main(String[] args) {
		int a = 3;
		int b = 5;
		int c = 7;
				
		int opcionImprimir = 2;
				switch (opcionImprimir){
				case 1:
					System.out.println("hola");
					break;
				case 2:
					System.out.println("Hasta Luegio");
					break;
				case 3:
					System.out.println("Agur");
				}

	}

}
