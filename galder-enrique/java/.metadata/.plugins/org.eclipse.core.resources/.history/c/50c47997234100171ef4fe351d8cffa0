package geometria;

public class Punto {

	private int coordX;
	private int coordY;

	public Punto(int coordX, int coordY) {
		this.coordX = coordX;
		this.coordY = coordY;

	}
	public Punto(){
		this.coordX = 0;
		this.coordY = 0;
	}
	public int getcoordX()
    {
    	return coordX;
    }
	public void setcoordX(int coordX)
    {
    	this.coordX = coordX;
    }
	
	public int getcoordY()
    {
    	return coordY;
    }
	
	public void setcoordY(int coordY)
    {
    	this.coordY = coordY;
    }
	
}
